#~ msgstr "&Adicionar"

#~ msgid "Remove"
#~ msgstr "Remover"

#~ msgid "C&hange Format..."
#~ msgstr "Modificar o &Formato..."

#~ msgid "C&lear Format"
#~ msgstr "&Limpar o Formato"

